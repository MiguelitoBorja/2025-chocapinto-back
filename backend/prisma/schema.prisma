generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  role     String

  clubs    Club[] @relation("ClubMembers")
}

model Club {
  id          Int    @id @default(autoincrement())
  name        String
  description String
  id_owner    Int
  members     User[] @relation("ClubMembers")
}
